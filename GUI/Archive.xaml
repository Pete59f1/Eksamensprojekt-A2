        <ComboBox x:Name="cbb_PlantType" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="621,156,0,0" SelectionChanged="Cbb_PlantType_SelectionChanged"/>
        <Label Content="Produktions Nummer" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="621,11,0,0" Width="133"/>
        <Label Content="Plante Sort" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="10.841,6.007" Margin="621,125,0,0" Width="120"/>

    </Grid>
</Window>
