        mc:Ignorable="d"
        Title="Archive" Height="450" Width="800">
    <Grid>
        <Button x:Name="btn_Search" Content="Søg" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="666,306,0,0" Click="Btn_Search_Click"/>
